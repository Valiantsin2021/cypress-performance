{
  "name": "cypress-performance",
  "version": "1.0.1",
  "description": "Cypress performance plugin to measure performance of UI of the application",
  "main": "src/commands/index.js",
  "types": "src/commands/performance.d.ts",
  "files": [
    "src/commands"
  ],
  "scripts": {
    "test": "cypress run",
    "badges": "npx -p dependency-version-badge update-badge cypress",
    "semantic-release": "semantic-release",
    "stop-only": "stop-only --folder cypress/e2e",
    "lint": "npx eslint \"**/*.js\" --fix",
    "format": "prettier --write \"**/*.js\"",
    "check": "npm run format && npm run lint"
  },
  "keywords": [
    "cypress-plugin",
    "cypress",
    "performance",
    "cy"
  ],
  "author": "Valentin Lutchanka <lutchenkovalentin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.17.0",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0",
    "stop-only": "^3.4.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "4.1.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.2.1"
  },
  "peerDependencies": {
    "cypress": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Valiantsin2021/cypress-performance.git"
  }
}